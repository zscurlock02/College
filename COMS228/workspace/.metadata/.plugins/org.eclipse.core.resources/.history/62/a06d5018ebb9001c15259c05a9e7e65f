
public class problem7 {
	public static void main(String[] args) {
		mystery(10);
	}
	public static void mystery(int x){ //10
	  if (x == 1) {
	    System.out.println("pooh");
	  }
	  else if (x % 2 == 0) // x = 10, x = 5
	  {
	    System.out.println(x);    
	    mystery(x / 2);           
	  }
	  else
	  { 
	    mystery(x - 1);
	  }
	}
}
