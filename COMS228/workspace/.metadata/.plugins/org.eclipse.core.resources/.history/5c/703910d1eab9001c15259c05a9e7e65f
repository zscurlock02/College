
public class problem7 {
	public static void main(String[] args) {
		System.out.print(mystery(10));
	}
	public static void mystery(int x){
	  if (x == 1) {
	    System.out.println("pooh");
	  }
	  else if (x % 2 == 0)
	  {
	    System.out.println(x);    
	    mystery(x / 2);           
	  }
	  else
	  { 
	    mystery(x - 1);
	  }
	}
}
